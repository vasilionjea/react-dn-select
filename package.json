{
  "name": "react-dn-select",
  "version": "1.2.2",
  "type": "module",
  "author": "Vasili Onjea",
  "description": "React component to drag and select any rendered items",
  "keywords": [
    "react",
    "selectable",
    "react-select",
    "react-selectable",
    "react-dn-select",
    "react-drag-and-select"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasilionjea/react-dn-select.git"
  },
  "exports": {
    "default": "./index.es.js",
    "import": "./index.es.js",
    "require": "./index.umd.js",
    "types": "./index.d.ts"
  },
  "homepage": "https://vasilionjea.github.io/react-dn-select/",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist/",
    "release": "./scripts/release.sh",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:example": "vite build --config vite.example.config.ts --emptyOutDir",
    "predeploy": "npm run build:example",
    "deploy": "gh-pages -d example-dist"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1"
  }
}
